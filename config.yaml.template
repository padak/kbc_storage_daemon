# Keboola Storage Daemon Configuration

# CSV Processing
csv_dialect:
  delimiter: ","
  quotechar: '"'
  escapechar: "\\"
  encoding: "utf-8"

# Upload Settings
upload:
  compression_threshold_mb: 50  # Files larger than this will be gzipped
  max_retries: 3
  retry_delay: 5  # seconds

# Logging
log_rotation:
  max_size_mb: 100
  backup_count: 5  # Number of backup files to keep 